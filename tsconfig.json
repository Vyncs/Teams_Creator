{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "types": ["styled-components-react-native"],
    "baseUrl": "./",
    "paths": {
      "@assets/*":[
        "./src/assets/*"
      ],
      "@components/*":[
        "./src/components/*"
      ],
      "@screens/*":[
        "./src/screens/*"
      ],
      "@storage/*":[
        "./src/storage/*"
      ],
      "@theme/*":[
        "./src/theme/*"
      ],
      "@utils/*":[
        "./src/utils/*"
      ],
    }
  }
}
